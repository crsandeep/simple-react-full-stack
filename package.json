{
  "name": "mySpace",
  "version": "1.0.0",
  "description": "mySpace is a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "author": "Lenno",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "formik": "^2.1.4",
    "moment": "^2.24.0",
    "bootstrap": "^4.4.1",
    "express": "^4.16.3",
    "react": "^16.5.2",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-dom": "^16.5.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-datepicker": "^2.14.0",
    "react-moment": "^0.9.7",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "nodemon": "^1.17.3",
    "react-test-render": "^1.1.2",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/jest/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/jest/setupTest.js"
    ]
  }
}
